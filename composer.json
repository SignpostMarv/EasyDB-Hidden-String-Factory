{
    "name": "signpostmarv/easydb-hidden-string-factory",
    "description": "Pass HiddenString instances to EasyDb",
    "license": "MIT",
    "authors": [
        {
            "name": "SignpostMarv",
            "email": "me@signpostmarv.name"
        }
    ],
    "autoload": {
        "psr-4": {
            "SignpostMarv\\EasyDBHiddenString\\": "./src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ParagonIE\\EasyDB\\Tests\\": "./vendor/paragonie/easydb/tests/"
        }
    },
    "config": {
        "classmap-authoritative": true,
        "sort-packages": true
    },
    "require": {
        "php": "^7",
        "paragonie/easydb": "^2.9",
        "paragonie/hidden-string": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6|^7|^8",
        "psalm/plugin-phpunit": "^0.6.0",
        "squizlabs/php_codesniffer": "^3",
        "vimeo/psalm": "^3"
    }
}
